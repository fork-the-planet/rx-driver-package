<module xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" schemaVersion="2.0" xs:noNamespaceSchemaLocation="schema_mdf_2.00.xsd">
    <fileVersion>1.00</fileVersion>
    <formatVersion>2.00</formatVersion>
    <fileTemplate></fileTemplate>
    <configuration>
        <property id="USB_CFG_PCDC_BULK_IN" display="Setting the USB Bulk In pipe" default="USB_PIPE1" type="combobox">
            <option id="USB_CFG_PCDC_BULK_IN.USB_PIPE1" display="Using USB PIPE1" value="USB_PIPE1"/>
            <option id="USB_CFG_PCDC_BULK_IN.USB_PIPE2" display="Using USB PIPE2" value="USB_PIPE2"/>
            <option id="USB_CFG_PCDC_BULK_IN.USB_PIPE3" display="Using USB PIPE3" value="USB_PIPE3"/>
            <option id="USB_CFG_PCDC_BULK_IN.USB_PIPE4" display="Using USB PIPE4" value="USB_PIPE4"/>
            <option id="USB_CFG_PCDC_BULK_IN.USB_PIPE5" display="Using USB PIPE5" value="USB_PIPE5"/>
            <constraint display="Selected pipe must not be used multiple times" actionOnFail="error">("${USB_CFG_PCDC_BULK_IN}" != "${USB_CFG_PCDC_BULK_OUT}")</constraint>
            <description>Using USB PIPE1
Using USB PIPE2
Using USB PIPE3
Using USB PIPE4
Using USB PIPE5</description>
        </property>
        <property id="USB_CFG_PCDC_BULK_OUT" display="Setting the USB Bulk Out pipe" default="USB_PIPE2" type="combobox">
            <option id="USB_CFG_PCDC_BULK_OUT.USB_PIPE1" display="Using USB PIPE1" value="USB_PIPE1"/>
            <option id="USB_CFG_PCDC_BULK_OUT.USB_PIPE2" display="Using USB PIPE2" value="USB_PIPE2"/>
            <option id="USB_CFG_PCDC_BULK_OUT.USB_PIPE3" display="Using USB PIPE3" value="USB_PIPE3"/>
            <option id="USB_CFG_PCDC_BULK_OUT.USB_PIPE4" display="Using USB PIPE4" value="USB_PIPE4"/>
            <option id="USB_CFG_PCDC_BULK_OUT.USB_PIPE5" display="Using USB PIPE5" value="USB_PIPE5"/>
            <constraint display="Selected pipe must not be used multiple times" actionOnFail="error">("${USB_CFG_PCDC_BULK_OUT}" != "${USB_CFG_PCDC_BULK_IN}")</constraint>
            <description>Using USB PIPE1
Using USB PIPE2
Using USB PIPE3
Using USB PIPE4
Using USB PIPE5</description>
        </property>
        <property id="USB_CFG_PCDC_INT_IN" display="Setting the USB Interrupt In pipe." default="USB_PIPE6" type="combobox">
            <option id="USB_CFG_PCDC_INT_IN.USB_PIPE6" display="Using USB PIPE6" value="USB_PIPE6"/>
            <option id="USB_CFG_PCDC_INT_IN.USB_PIPE7" display="Using USB PIPE7" value="USB_PIPE7"/>
            <option id="USB_CFG_PCDC_INT_IN.USB_PIPE8" display="Using USB PIPE8" value="USB_PIPE8"/>
            <option id="USB_CFG_PCDC_INT_IN.USB_PIPE9" display="Using USB PIPE9" value="USB_PIPE9"/>
            <description>Using USB PIPE6
Using USB PIPE7
Using USB PIPE8
Using USB PIPE9</description>
        </property>
    </configuration>
</module>
