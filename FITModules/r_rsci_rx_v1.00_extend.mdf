    <configuration>
        <property id="RSCI_CFG_PARAM_CHECKING_ENABLE" display="Parameter checking" default="BSP_CFG_PARAM_CHECKING_ENABLE" type="combobox">
            <option id="RSCI_CFG_PARAM_CHECKING_ENABLE.BSP_CFG_PARAM_CHECKING_ENABLE" display="System Default" value="BSP_CFG_PARAM_CHECKING_ENABLE"/>
            <option id="RSCI_CFG_PARAM_CHECKING_ENABLE.0" display="Not" value="0"/>
            <option id="RSCI_CFG_PARAM_CHECKING_ENABLE.1" display="Include" value="1"/>
            <description>Selects whether to include parameter checking in the code.
BSP_CFG_PARAM_CHECKING_ENABLE = System default.
0 = compile out parameter checking.
1 = includes parameter checking.</description>
        </property>
        <property id="RSCI_CFG_ASYNC_INCLUDED" display="Use ASYNC mode" default="1" type="combobox">
            <option id="RSCI_CFG_ASYNC_INCLUDED.0" display="Not" value="0"/>
            <option id="RSCI_CFG_ASYNC_INCLUDED.1" display="Include" value="1"/>
            <description>SPECIFY WHETHER TO INCLUDE CODE FOR DIFFERENT RSCI MODES 
Setting an equate to 1 includes code specific to that mode. </description>
        </property>
        <property id="RSCI_CFG_SYNC_INCLUDED" display="Use SYNC mode" default="0" type="combobox">
            <option id="RSCI_CFG_SYNC_INCLUDED.0" display="Not" value="0"/>
            <option id="RSCI_CFG_SYNC_INCLUDED.1" display="Include" value="1"/>
            <description>SPECIFY WHETHER TO INCLUDE CODE FOR DIFFERENT RSCI MODES 
Setting an equate to 1 includes code specific to that mode. </description>
