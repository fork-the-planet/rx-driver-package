<module xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" schemaVersion="2.0" xs:noNamespaceSchemaLocation="schema_mdf_2.00.xsd">
  <fileVersion>1.00</fileVersion>
  <formatVersion>2.00</formatVersion>
  <fileTemplate>
  </fileTemplate>
  <configuration>
    <property id="RM_OB1203_CFG_PARAM_CHECKING_ENABLE" display="Parameter Checking" default="BSP_CFG_PARAM_CHECKING_ENABLE" type="combobox">
      <option id="RM_OB1203_CFG_PARAM_CHECKING_ENABLE.BSP_CFG_PARAM_CHECKING_ENABLE" display="System Default" value="BSP_CFG_PARAM_CHECKING_ENABLE" />
      <option id="RM_OB1203_CFG_PARAM_CHECKING_ENABLE.0" display="Disabled" value="0" />
      <option id="RM_OB1203_CFG_PARAM_CHECKING_ENABLE.1" display="Enabled" value="1" />
      <description>Select whether to include parameter checking in the code.
"Disabled": Compiles out parameter checking.
"Enabled": Includes parameter checking.
"System Default": Sets this to "BSP_CFG_PARAM_CHECKING_ENABLE"</description>
    </property>
    <property id="RM_OB1203_CFG_DEVICE_NUM_MAX" display="Number of OB1203 Sensors" default="1" type="combobox">
      <option id="RM_OB1203_CFG_DEVICE_NUM_MAX.1" display="1" value="1" />
      <option id="RM_OB1203_CFG_DEVICE_NUM_MAX.2" display="2" value="2" />
      <description>Set the number of OB1203 Sensors controlled by the FIT module.
[Note]: Up to 2 OB1203 sensors can be controlled.</description>
    </property>

    <!-- Device0 -->
    <property id="RM_OB1203_CFG_DEVICE0_SENSOR_MODE" display="Sensor mode of OB1203 Sensor device0" default="0" type="combobox">
      <option id="RM_OB1203_CFG_DEVICE0_SENSOR_MODE.0" display="Not selected" value="0" />
      <option id="RM_OB1203_CFG_DEVICE0_SENSOR_MODE.1" display="Light sensor mode" value="1" />
      <option id="RM_OB1203_CFG_DEVICE0_SENSOR_MODE.2" display="Proximity sensor mode" value="2" />
      <option id="RM_OB1203_CFG_DEVICE0_SENSOR_MODE.3" display="Light Proximity sensor mode" value="3" />
      <option id="RM_OB1203_CFG_DEVICE0_SENSOR_MODE.4" display="PPG sensor mode" value="4" />
      <description>Set the sensor mode of OB1203 Sensors controlled by the FIT module.
"Light sensor mode": Light sensor mode.
"Proximity sensor mode": Proximity sensor mode.
"Light Proximity sensor mode": Light sensor and Proximity sensor mode.
"PPG sensor mode": PPG sensor mode.</description>
    </property>
    <property id="RM_OB1203_CFG_DEVICE0_COMMS_INSTANCE" display="I2C Communication device No. for OB1203 sensor device0" default="g_comms_i2c_device0" type="combobox">
      <option id="RM_OB1203_CFG_DEVICE0_COMMS_INSTANCE.1" display="I2C Communication Device0" value="g_comms_i2c_device0" />
      <option id="RM_OB1203_CFG_DEVICE0_COMMS_INSTANCE.2" display="I2C Communication Device1" value="g_comms_i2c_device1" />
      <option id="RM_OB1203_CFG_DEVICE0_COMMS_INSTANCE.3" display="I2C Communication Device2" value="g_comms_i2c_device2" />
      <option id="RM_OB1203_CFG_DEVICE0_COMMS_INSTANCE.4" display="I2C Communication Device3" value="g_comms_i2c_device3" />
      <option id="RM_OB1203_CFG_DEVICE0_COMMS_INSTANCE.5" display="I2C Communication Device4" value="g_comms_i2c_device4" />
      <option id="RM_OB1203_CFG_DEVICE0_COMMS_INSTANCE.6" display="I2C Communication Device5" value="g_comms_i2c_device5" />
      <option id="RM_OB1203_CFG_DEVICE0_COMMS_INSTANCE.7" display="I2C Communication Device6" value="g_comms_i2c_device6" />
      <option id="RM_OB1203_CFG_DEVICE0_COMMS_INSTANCE.8" display="I2C Communication Device7" value="g_comms_i2c_device7" />
      <option id="RM_OB1203_CFG_DEVICE0_COMMS_INSTANCE.9" display="I2C Communication Device8" value="g_comms_i2c_device8" />
      <option id="RM_OB1203_CFG_DEVICE0_COMMS_INSTANCE.10" display="I2C Communication Device9" value="g_comms_i2c_device9" />
      <option id="RM_OB1203_CFG_DEVICE0_COMMS_INSTANCE.11" display="I2C Communication Device10" value="g_comms_i2c_device10" />
      <option id="RM_OB1203_CFG_DEVICE0_COMMS_INSTANCE.12" display="I2C Communication Device11" value="g_comms_i2c_device11" />
      <option id="RM_OB1203_CFG_DEVICE0_COMMS_INSTANCE.13" display="I2C Communication Device12" value="g_comms_i2c_device12" />
      <option id="RM_OB1203_CFG_DEVICE0_COMMS_INSTANCE.14" display="I2C Communication Device13" value="g_comms_i2c_device13" />
      <option id="RM_OB1203_CFG_DEVICE0_COMMS_INSTANCE.15" display="I2C Communication Device14" value="g_comms_i2c_device14" />
      <option id="RM_OB1203_CFG_DEVICE0_COMMS_INSTANCE.16" display="I2C Communication Device15" value="g_comms_i2c_device15" />
      <constraint display="This I2C communication device is used by other device.">("${RM_OB1203_CFG_DEVICE_NUM_MAX}" == "${RM_OB1203_CFG_DEVICE_NUM_MAX.1}" || "${RM_OB1203_CFG_DEVICE0_COMMS_INSTANCE}" != "${RM_OB1203_CFG_DEVICE1_COMMS_INSTANCE}")</constraint>
      <description>Set the using I2C communication device number for OB1203 Sensors controlled by the FIT module.
[Note]: Be sure to specify a valid I2C communication device number.</description>
    </property>
    <property id="RM_OB1203_CFG_DEVICE0_COMMS_I2C_CALLBACK" display="I2C callback function for OB1203 sensor device0" default="ob1203_user_i2c_callback0" type="textbox">
      <constraint display="Must not be empty">"" != "${RM_OB1203_CFG_DEVICE0_COMMS_I2C_CALLBACK}"</constraint>
      <description>Specify the I2C Callback function name of the OB1203 Sensor API.</description>
    </property>
    <property id="RM_OB1203_CFG_DEVICE0_IRQ_ENABLE" display="Enable IRQ from OB1203 sensor device0" default="0" type="combobox">
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_ENABLE.0" display="Disabled" value="0" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_ENABLE.1" display="Enabled" value="1" />
      <description>Specify the Enalbe IRQ.
- When Disabled, Don't enable IRQ"
- When Enabled, enable the IRQ"</description>
    </property>
    <property id="RM_OB1203_CFG_DEVICE0_IRQ_CALLBACK" display="IRQ Callback function for OB1203 sensor device0" default="ob1203_user_irq_callback0" type="textbox">
      <constraint display="When IRQ is enabled" actionOnFail="disable">${RM_OB1203_CFG_DEVICE0_IRQ_ENABLE} == ${RM_OB1203_CFG_DEVICE0_IRQ_ENABLE.1}</constraint>
      <constraint display="Must not be empty">"" != "${RM_OB1203_CFG_DEVICE0_IRQ_CALLBACK}"</constraint>
      <description>Specify the IRQ Callback function name of the OB1203 Sensor API.</description>
    </property>
    <property id="RM_OB1203_CFG_DEVICE0_IRQ_NUMBER" display="IRQ number for OB1203 sensor device0" default="IRQ_NUM_0" type="combobox">
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_NUMBER.0" display="IRQ0" value="IRQ_NUM_0" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_NUMBER.1" display="IRQ1" value="IRQ_NUM_1" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_NUMBER.2" display="IRQ2" value="IRQ_NUM_2" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_NUMBER.3" display="IRQ3" value="IRQ_NUM_3" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_NUMBER.4" display="IRQ4" value="IRQ_NUM_4" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_NUMBER.5" display="IRQ5" value="IRQ_NUM_5" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_NUMBER.6" display="IRQ6" value="IRQ_NUM_6" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_NUMBER.7" display="IRQ7" value="IRQ_NUM_7" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_NUMBER.8" display="IRQ8" value="IRQ_NUM_8" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_NUMBER.9" display="IRQ9" value="IRQ_NUM_9" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_NUMBER.10" display="IRQ10" value="IRQ_NUM_10" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_NUMBER.11" display="IRQ11" value="IRQ_NUM_11" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_NUMBER.12" display="IRQ12" value="IRQ_NUM_12" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_NUMBER.13" display="IRQ13" value="IRQ_NUM_13" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_NUMBER.14" display="IRQ14" value="IRQ_NUM_14" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_NUMBER.15" display="IRQ15" value="IRQ_NUM_15" />
      <constraint display="When IRQ is enabled" actionOnFail="disable">${RM_OB1203_CFG_DEVICE0_IRQ_ENABLE} == ${RM_OB1203_CFG_DEVICE0_IRQ_ENABLE.1}</constraint>
      <description>Set the IRQ number for the OB1203 Sensor.
[Note]: Be sure to specify a valid IRQ number.</description>
    </property>
    <property id="RM_OB1203_CFG_DEVICE0_IRQ_TRIGGER" display="IRQ trigger for OB1203 sensor device0" default="IRQ_TRIG_RISING" type="combobox">
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_TRIGGER.lowLevel" display="Low Level" value="IRQ_TRIG_LOWLEV" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_TRIGGER.falling" display="Falling" value="IRQ_TRIG_FALLING" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_TRIGGER.rising" display="Rising" value="IRQ_TRIG_RISING" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_TRIGGER.bothEdges" display="Both Edges" value="IRQ_TRIG_BOTH_EDGE" />
      <constraint display="When IRQ is enabled" actionOnFail="disable">${RM_OB1203_CFG_DEVICE0_IRQ_ENABLE} == ${RM_OB1203_CFG_DEVICE0_IRQ_ENABLE.1}</constraint>
      <constraint display="When using one or more OB1203 sensor devices" actionOnFail="disable">
      "${RM_OB1203_CFG_DEVICE_NUM_MAX}" == "${RM_OB1203_CFG_DEVICE_NUM_MAX.1}" 
      || "${RM_OB1203_CFG_DEVICE_NUM_MAX}" == "${RM_OB1203_CFG_DEVICE_NUM_MAX.2}"
      </constraint>
      <description>Set the IRQ trigger for the OB1203 Sensor.</description>
    </property>
    <property id="RM_OB1203_CFG_DEVICE0_IRQ_PRIORITY" display="IRQ interrupt priority for OB1203 sensor device0" default="IRQ_PRI_10" type="combobox">
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_PRIORITY.0" display="Priority 0" value="IRQ_PRI_0" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_PRIORITY.1" display="Priority 1" value="IRQ_PRI_1" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_PRIORITY.2" display="Priority 2" value="IRQ_PRI_2" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_PRIORITY.3" display="Priority 3" value="IRQ_PRI_3" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_PRIORITY.4" display="Priority 4" value="IRQ_PRI_4" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_PRIORITY.5" display="Priority 5" value="IRQ_PRI_5" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_PRIORITY.6" display="Priority 6" value="IRQ_PRI_6" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_PRIORITY.7" display="Priority 7" value="IRQ_PRI_7" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_PRIORITY.8" display="Priority 8" value="IRQ_PRI_8" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_PRIORITY.9" display="Priority 9" value="IRQ_PRI_9" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_PRIORITY.10" display="Priority 10" value="IRQ_PRI_10" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_PRIORITY.11" display="Priority 11" value="IRQ_PRI_11" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_PRIORITY.12" display="Priority 12" value="IRQ_PRI_12" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_PRIORITY.13" display="Priority 13" value="IRQ_PRI_13" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_PRIORITY.14" display="Priority 14" value="IRQ_PRI_14" />
      <option id="RM_OB1203_CFG_DEVICE0_IRQ_PRIORITY.15" display="Priority 15" value="IRQ_PRI_15" />
      <constraint display="When IRQ is enabled" actionOnFail="disable">${RM_OB1203_CFG_DEVICE0_IRQ_ENABLE} == ${RM_OB1203_CFG_DEVICE0_IRQ_ENABLE.1}</constraint>
      <description>Set the IRQ interrupt priority for the OB1203 Sensor.</description>
    </property>
    <property id="RM_OB1203_CFG_DEVICE0_SEMAPHORE_TIMEOUT" display="Semaphore timeout for OB1203 sensor device0 (RTOS only)" default="0xFFFFFFFF" type="textbox">
      <constraint display="Must be integer value (0x00 - 0xFFFFFFFF)">((0x00 &lt;= ${RM_OB1203_CFG_DEVICE0_SEMAPHORE_TIMEOUT}) &amp;&amp; (0xFFFFFFFF &gt;= ${RM_OB1203_CFG_DEVICE0_SEMAPHORE_TIMEOUT}))</constraint>
      <description>Specify the semaphore timeout for OB1203 sensor device0.</description>
    </property>
     <!-- General -->
    <property id="RM_OB1203_CFG_DEVICE0_LIGHT_PROX_DEVICE_INTERRUPT" display="General : Device interrupt for Light Proximity mode of OB1203 sensor device0" default="RM_OB1203_OPERATION_MODE_STANDBY" type="combobox">
      <option id="RM_OB1203_CFG_DEVICE0_LIGHT_PROX_DEVICE_INTERRUPT.1" display="Light mode interrupt" value="RM_OB1203_OPERATION_MODE_LIGHT" />
      <option id="RM_OB1203_CFG_DEVICE0_LIGHT_PROX_DEVICE_INTERRUPT.2" display="Proximity mode interrupt" value="RM_OB1203_OPERATION_MODE_PROXIMITY" />
      <constraint display="When Light Proximity is enabled" actionOnFail="disable">
      "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE}" == "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE.3}"
      </constraint>
      <constraint display="When IRQ is enabled" actionOnFail="disable">${RM_OB1203_CFG_DEVICE0_IRQ_ENABLE} == ${RM_OB1203_CFG_DEVICE0_IRQ_ENABLE.1}</constraint>
      <description>Specify the operation mode using device interrupt for the OB1203 Sensor.</description>
    </property>
    <property id="RM_OB1203_CFG_DEVICE0_PPG_PROX_GAIN" display="General : Gain for Proximity mode and PPG mode of OB1203 sensor device0" default="RM_OB1203_PPG_PROX_GAIN_1" type="combobox">
      <option id="RM_OB1203_CFG_DEVICE0_PPG_PROX_GAIN.0" display="1" value="RM_OB1203_PPG_PROX_GAIN_1" />
      <option id="RM_OB1203_CFG_DEVICE0_PPG_PROX_GAIN.1" display="1.5" value="RM_OB1203_PPG_PROX_GAIN_1P5" />
      <option id="RM_OB1203_CFG_DEVICE0_PPG_PROX_GAIN.2" display="2" value="RM_OB1203_PPG_PROX_GAIN_2" />
      <option id="RM_OB1203_CFG_DEVICE0_PPG_PROX_GAIN.3" display="4" value="RM_OB1203_PPG_PROX_GAIN_4" />
      <constraint display="When Proximity, Light Proximity and PPG mode is enabled" actionOnFail="disable">
      "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE}" == "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE.2}" 
      || "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE}" == "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE.3}"
      || "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE}" == "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE.4}"
      </constraint>
      <description>Specify the gain of ADC output and noise for the Proximity mode and PPG mode.</description>
    </property>
    <property id="RM_OB1203_CFG_DEVICE0_LED_ORDER" display="General : LED order of OB1203 sensor device0" default="RM_OB1203_LED_IR_FIRST_RED_SECOND" type="combobox">
      <option id="RM_OB1203_CFG_DEVICE0_LED_ORDER.1" display="IR LED first, Red LED second" value="RM_OB1203_LED_IR_FIRST_RED_SECOND" />
      <option id="RM_OB1203_CFG_DEVICE0_LED_ORDER.2" display="Red LED first, IR LED second" value="RM_OB1203_LED_RED_FIRST_IR_SECOND" />
      <constraint display="When Proximity, Light Proximity and PPG mode is enabled" actionOnFail="disable">
      "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE}" == "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE.2}" 
      || "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE}" == "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE.3}"
      || "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE}" == "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE.4}"
      </constraint>
      <description>Specify the LED order of the OB1203 Sensor.</description>
    </property>
    <!-- Light mode -->
    <property id="RM_OB1203_CFG_DEVICE0_LIGHT_SENSOR_MODE" display="Light mode : Operation mode for ob1203 sensor device0" default="RM_OB1203_LIGHT_SENSOR_MODE_LS" type="combobox">
      <option id="RM_OB1203_CFG_DEVICE0_LIGHT_SENSOR_MODE.1" display="LS mode" value="RM_OB1203_LIGHT_SENSOR_MODE_LS" />
      <option id="RM_OB1203_CFG_DEVICE0_LIGHT_SENSOR_MODE.2" display="CS mode" value="RM_OB1203_LIGHT_SENSOR_MODE_CS" />
      <constraint display="When Light and Light Proximity is enabled" actionOnFail="disable">
      "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE}" == "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE.1}" 
      || "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE}" == "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE.3}"
      </constraint>
      <description>Specify the operation mode for the OB1203 Sensor.</description>
    </property>
    <property id="RM_OB1203_CFG_DEVICE0_LIGHT_INTERRUPT_TYPE" display="Light mode : Interrupt type for OB1203 sensor device0" default="RM_OB1203_LIGHT_INTERRUPT_TYPE_THRESHOLD" type="combobox">
      <option id="RM_OB1203_CFG_DEVICE0_LIGHT_INTERRUPT_TYPE.1" display="Threshold" value="RM_OB1203_LIGHT_INTERRUPT_TYPE_THRESHOLD" />
      <option id="RM_OB1203_CFG_DEVICE0_LIGHT_INTERRUPT_TYPE.2" display="Variation" value="RM_OB1203_LIGHT_INTERRUPT_TYPE_VARIATION" />
      <constraint display="When Light and Light Proximity is enabled" actionOnFail="disable">
      "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE}" == "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE.1}" 
      || "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE}" == "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE.3}"
      </constraint>
      <description>Specify the interrupt type for the OB1203 Sensor.</description>
    </property>
    <property id="RM_OB1203_CFG_DEVICE0_LIGHT_INTERRUPT_SOURCE" display="Light mode : Interrupt source for OB1203 sensor device0" default="RM_OB1203_LIGHT_INTERRUPT_SOURCE_CLEAR_CHANNEL" type="combobox">
      <option id="RM_OB1203_CFG_DEVICE0_LIGHT_INTERRUPT_SOURCE.1" display="Clear channel" value="RM_OB1203_LIGHT_INTERRUPT_SOURCE_CLEAR_CHANNEL" />
      <option id="RM_OB1203_CFG_DEVICE0_LIGHT_INTERRUPT_SOURCE.2" display="Green channel" value="RM_OB1203_LIGHT_INTERRUPT_SOURCE_GREEN_CHANNEL" />
      <option id="RM_OB1203_CFG_DEVICE0_LIGHT_INTERRUPT_SOURCE.3" display="Red channel (CS mode only)" value="RM_OB1203_LIGHT_INTERRUPT_SOURCE_RED_CHANNEL" />
      <option id="RM_OB1203_CFG_DEVICE0_LIGHT_INTERRUPT_SOURCE.4" display="Blue channel (CS mode only)" value="RM_OB1203_LIGHT_INTERRUPT_SOURCE_BLUE_CHANNEL" />
      <constraint display="When Light and Light Proximity is enabled" actionOnFail="disable">
      "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE}" == "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE.1}" 
      || "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE}" == "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE.3}"
      </constraint>
      <description>Specify the interrupt source for the OB1203 Sensor.</description>
    </property>
    <property id="RM_OB1203_CFG_DEVICE0_LIGHT_INTERRUPT_PERSIST" display="Light mode : The number of Similar Consecutive Interrupt Events of Light mode for OB1203 sensor device0" default="0x2" type="textbox">
      <constraint display="When Light and Light Proximity is enabled" actionOnFail="disable">
      "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE}" == "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE.1}" 
      || "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE}" == "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE.3}"
      </constraint>
      <constraint display="Value must be a non-negative integer">0 &lt;= "${RM_OB1203_CFG_DEVICE0_LIGHT_INTERRUPT_PERSIST}"</constraint>
      <constraint display="Value must be 4bits or less">0xF &gt;= "${RM_OB1203_CFG_DEVICE0_LIGHT_INTERRUPT_PERSIST}"</constraint>
      <description>Specify the number of similar consecutive Light mode interrupt events that must occur before the interrupt is asserted (4bits).
Min = 0x00 and Max 0x0F.</description>
    </property>
    <property id="RM_OB1203_CFG_DEVICE0_LIGHT_SLEEP_AFTER_INTERRUPT" display="Light mode : Sleep after interrupt for OB1203 sensor device0" default="RM_OB1203_SLEEP_AFTER_INTERRUPT_DISABLE" type="combobox">
      <option id="RM_OB1203_CFG_DEVICE0_LIGHT_SLEEP_AFTER_INTERRUPT.0" display="Disabled" value="RM_OB1203_SLEEP_AFTER_INTERRUPT_DISABLE" />
      <option id="RM_OB1203_CFG_DEVICE0_LIGHT_SLEEP_AFTER_INTERRUPT.1" display="Enabled" value="RM_OB1203_SLEEP_AFTER_INTERRUPT_ENABLE" />
      <constraint display="When Light and Light Proximity is enabled" actionOnFail="disable">
      "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE}" == "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE.1}" 
      || "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE}" == "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE.3}"
      </constraint>
      <description>Specify the sleep after interrupt for the OB1203 Sensor.</description>
    </property>
    <property id="RM_OB1203_CFG_DEVICE0_LIGHT_GAIN" display="Light mode : Gain of OB1203 sensor device0" default="RM_OB1203_LIGHT_GAIN_1" type="combobox">
      <option id="RM_OB1203_CFG_DEVICE0_LIGHT_GAIN.1" display="1" value="RM_OB1203_LIGHT_GAIN_1" />
      <option id="RM_OB1203_CFG_DEVICE0_LIGHT_GAIN.2" display="3" value="RM_OB1203_LIGHT_GAIN_3" />
      <option id="RM_OB1203_CFG_DEVICE0_LIGHT_GAIN.3" display="6" value="RM_OB1203_LIGHT_GAIN_6" />
      <constraint display="When Light and Light Proximity is enabled" actionOnFail="disable">
      "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE}" == "${RM_OB1203_CFG_DEVICE0_SENSOR_MODE.1}" 
