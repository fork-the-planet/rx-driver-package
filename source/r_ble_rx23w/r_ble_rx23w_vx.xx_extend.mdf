<module xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" schemaVersion="2.16" xs:noNamespaceSchemaLocation="schema_mdf_2.16.xsd">
    <fileVersion>1.00</fileVersion>
    <formatVersion>2.16</formatVersion>
    <resourceInfo>
      <peripheral name="BLE">
        <pin name="CLKOUT_RF">
          <constraint display="CLKOUT_RF valid if BLE_CFG_RF_CLKOUT_EN is not 0." actionOnFail="disable">${BLE_CFG_RF_CLKOUT_EN} != ${BLE_CFG_RF_CLKOUT_EN.0}</constraint>
