        <version>8.03.00.202311</version>
    </toolchain>
    <toolchain>
        <brand>IAR</brand>
        <product>ICCRX</product>
        <version>5.10.01</version>
    </toolchain>

    <!-- only one target entry allowed; multiple sub tags permitted -->
    <target>
        <family>RX</family>
        <series>RX200</series>
        <series>RX600</series>
        <series>RX700</series>
        <group>RX231</group>
        <group>RX23W</group>
        <group>RX26T</group>
        <group>RX651</group>
        <group>RX65N</group>
        <group>RX66N</group>
        <group>RX66T</group>
        <group>RX671</group>
        <group>RX72M</group>
        <group>RX72N</group>
        <group>RX72T</group>
        <board>RSKRX231</board>
        <board>RSSKRX23W</board>
        <board>MCB-RX26T</board>
        <board>RSKRX65N_2MB</board>
        <board>RSKRX66T</board>
        <board>RSKRX671</board>
        <board>RSKRX72M</board>
        <board>RSKRX72N</board>
        <board>RSKRX72T</board>
    </target>

    <!-- only one package entry allowed -->
    <!-- multiple dependencies, applications, functions, and incdirs permitted -->
    <package>
        <type>fitmodule</type>
        <name>r_tsip_rx</name>
        <version>x.xx</version>
        <zipsource>r_tsip_rx_vx.xx.zip</zipsource>
        <dependency>
            <module>r_bsp</module>
            <version>7.30</version>
        </dependency>
        <application>Generic</application>
        <desc>TSIP(Trusted Secure IP) driver.</desc>
        <details>Support functions: AES, GCM, CCM, CMAC, SHA, MD5, Triple-DES, ARC4, RSA, ECC, Random number generate, Key management, secure boot/secure firmware update.</details>
        <function>TSIP(Trusted Secure IP) driver.</function>
        <incdir>r_config</incdir>
        <incdir>r_tsip_rx</incdir>
        <section>
            <address>0x00100000</address>
            <name>C_FIRMWARE_UPDATE_CONTROL_BLOCK</name>
        </section>
        <section>
            <address>0x00100000</address>
            <name>C_FIRMWARE_UPDATE_CONTROL_BLOCK_MIRROR</name>
        </section>
    </package>

</refinfo>
